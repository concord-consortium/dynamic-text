{
  "name": "@concord-consortium/dynamic-text",
  "version": "1.0.6",
  "description": "Dynamic text manager and React component",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "commonjs",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "run-s build:tsc build:rollup",
    "build:tsc": "tsc",
    "build:rollup": "rollup --config",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/dynamic-text.git"
  },
  "author": "Doug Martin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/dynamic-text/issues"
  },
  "homepage": "https://github.com/concord-consortium/dynamic-text#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/uuid": "^9.0.1",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.17.2",
    "rollup-plugin-dts": "^5.2.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@concord-consortium/lara-interactive-api": ">=1.8.0",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  }
}
